{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python: Current File",
            "type": "python",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal"
        },
        {
            "name": "Test_Benchmark",
            "type": "python",
            "request": "launch",
            "program": "${file}",
            "args": [
                "--file",
                "benchmark_small.csv",
                "--max_samples",
                "10000"
            ],
            "console": "integratedTerminal"
        },
        {
            "name": "Test",
            "type": "python",
            "request": "launch",
            "program": "${file}",
            "args": [
                "--config",
                "configs/celebA/ddpm.py",
                "--log_path",
                "./test_logs",
                "--debug",
                "True"
            ],
            "console": "integratedTerminal"
        },
        {
            "name": "Resume_Test",
            "type": "python",
            "request": "launch",
            "program": "${file}",
            "args": [
                "--config",
                "test_logs/ve/config.pkl",
                "--log_path",
                "./test_logs",
                "--checkpoint_path",
                "test_logs/ve/checkpoints/best/last.ckpt",
                "--debug",
                "True"
            ],
            "console": "integratedTerminal"
        }
    ]
}